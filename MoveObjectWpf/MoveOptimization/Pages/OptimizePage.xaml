<Page x:Class="MoveOptimization.Pages.OptimizePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      mc:Ignorable="d"
      Width="Auto"
      Height="Auto"
      MinHeight="200"
      MinWidth="200"
	Title="OptimizePage">

    <Grid>
        <ToggleButton Name="ButtonOptimize" Content="Start" IsChecked="False" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="ButtonOptimize_Click" />
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Stretch" Margin="20,50,20,20" Stroke="Gray" VerticalAlignment="Stretch" MouseLeave="Rectangle_MouseLeave" MouseEnter="Rectangle_MouseEnter" MouseMove="Rectangle_MouseMove" />
    </Grid>
</Page>